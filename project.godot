config_version=5

[application]

config/name="SpaceRisk"
run/main_scene="res://levels/main.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
run/max_fps=75
config/icon="res://icon.svg"

[autoload]

LaunchArgsSystem="*res://scenes/system/launch_args.tscn"
SteamSystem="*res://scenes/system/steam.tscn"
NetworkSystem="*res://scenes/system/network.tscn"
PlayerSpawnerSystem="*res://scenes/system/player/spawner.tscn"
UISystem="*res://scenes/system/ui.tscn"
CommanderSystem="*res://scenes/system/commander.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor]

run/main_run_args="--server --nosteam"

[editor_plugins]

enabled=PackedStringArray("res://addons/ply/plugin.cfg", "res://addons/simplegrasstextured/plugin.cfg")

[filesystem]

import/blender/enabled=false

